<section class="section">
  <h2 class="title is-5">Dates Logged</h2>
  <form id="category" method="POST" >
    <table class="table" style="width: 100%">
      <thead>
      <th>Date</th>
      <th></th>
      </thead>
      <tbody>
      {{# each datesLoggedByUser}}
        <tr>
          <td id="{{this}}">
            {{this}}
          </td>
          <td>
            <a class="button is-small" href="/date/{{this}}"> View </a>
          </td>
        </tr>
      {{/each}}
      </tbody>
    </table>
  </form>
</section>