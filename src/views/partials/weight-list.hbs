<section class="section">
  <h2 class="title is-5">Weights</h2>
    <table class="table" style="width: 100%">
      <thead>
      <th>Date</th>
      <th>Weight</th>
      </thead>
      <tbody>
      {{# each weights}}
        <tr>
          <td>{{date}}</td>
          <td>
            {{weight}}</td>
        </tr>
      {{/each}}
      </tbody>
    </table>
</section>